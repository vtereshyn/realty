{
  "name": "realty",
  "private": true,
  "author": "Vlad Tereshyn",
  "engines": {
    "node": ">=14.16.1"
  },
  "workspaces": {
    "packages": [
      "packages/**"
    ]
  },
  "scripts": {
    "build": "run-s build:*",
    "build:client": "lerna exec --scope='client' -- yarn build",
    "build:server": "lerna exec --scope='server' -- yarn build",
    "start": "run-s start:*",
    "start:client": "lerna exec --scope='client' -- yarn start",
    "start:server": "lerna exec --scope='server' -- yarn start:dev",
    "db:migrate": "lerna exec --scope='server' -- yarn db:migrate",
    "db:seed": "lerna exec --scope='server' -- yarn db:seed",
    "lint": "eslint 'packages/**/*.ts' --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docker:start": "docker-compose --env-file packages/server/.env up sql redis",
    "gql:generate": "lerna exec --scope='client' -- yarn gql:generate"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.8",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-react-apollo": "2.3.1",
    "@nestjs/cli": "8.0.2",
    "@nestjs/schematics": "8.0.2",
    "@nestjs/testing": "8.0.4",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.13",
    "@types/google-map-react": "2.1.2",
    "@types/google.maps": "3.45.6",
    "@types/node": "14.17.6",
    "@types/node-fetch": "2.5.12",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/pluralize": "0.0.29",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "eslint": "7.31.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-compat": "3.11.1",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-unicorn": "34.0.1",
    "graphql": "15.5.1",
    "lerna": "4.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "prettier-eslint": "12.0.0",
    "rimraf": "3.0.2",
    "ts-loader": "9.2.4",
    "ts-node": "10.1.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
